{
  "name": "cordova-plugin-amap",
  "version": "2.0.1",
  "description": "Amap Maps native SDK for Android and iOS.\nThis plugin allows your application to use the native Amap Maps views instead of the Amap Maps JavaScript API v3.\nThe native map view is faster, use less network usage, display 3D views, and even work on offline network.",
  "cordova": {
    "id": "cordova-plugin-amap",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silverbase-FE/cordova-plugin-amap.git"
  },
  "keywords": [
    "amap",
    "maps",
    "geo",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios"
  ],
  "engines": [
    {
      "name": "cordova",
      "version": ">=7.1.0"
    },
    {
      "name": "cordova-android",
      "version": ">=5.1.0"
    },
    {
      "name": "cordova-ios",
      "version": ">=4.0.0"
    },
    {
      "name": "apple-xcode",
      "version": ">=9.0.0"
    },
    {
      "name": "apple-ios",
      "version": ">=10.0.0"
    }
  ],
  "author": "piaoxiang",
  "license": "Apache 2.0 License",
  "bugs": {
    "url": "https://github.com/Silverbase-FE/cordova-plugin-amap/issues"
  },
  "homepage": "https://github.com/Silverbase-FE/cordova-plugin-amap#readme"
}
